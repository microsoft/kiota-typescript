        "@microsoft/kiota-authentication-azure": [
            "../authentication/azure"
        ],"@microsoft/kiota-authentication-spfx": [
            "../authentication/spfx"
        ],
    },
	"include": ["./generatedCode/**/*.ts", "./tests/**/*.ts"]
